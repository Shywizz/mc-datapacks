    function src.game.host(){
        execute unless entity @a[tag=host,limit=1] run tag @s add host;
        tag @a[tag=!player] add player;
}
    function src.game.restart(){
        execute as @e[tag=twd_game,tag=!twd_permanent] at @s run {
            data merge entity @s {DeathLootTable:"minecraft:empty",Silent:1b,Air:0};
            tp @s ~ -100 ~;
            kill @s;
        }
    src.game.start();
    }
    function src.game.game_over(){
        Text.subtitle(@a[tag=twd_game.player],"&<italic,white>You managed to fend off the enemies for &<globals:gametime>");
        Text.title(@a[tag=twd_game.player],"&<bold,dark_red>Game over");
    }
class src.game.code{
    function main(){
        execute as @a[tag=twd_game.player] run stopsound @s * minecraft:block.portal.ambient;
        execute as @a[x=35,y=23,z=-27,dx=10,dy=7,dz=13,tag=!twd_game.shop] run tag @s add twd_game.shop;
        execute as @a[tag=twd_game.shop] unless entity @s[x=35,y=23,z=-27,dx=10,dy=7,dz=13] run tag @s remove twd_game.shop;
        execute as @a[tag=twd_game.shop] run src.game.code.shop();
    }
    function read_parser(){
        execute as @e[type=armor_stand,tag=parser] at @s unless block ~ ~-.35 ~ chest{Items:[]} run {
        scoreboard players add @s wave 1;
        execute if score @s wave matches 5.. at @s run playsound block.note_block.harp master @s;
        execute if score @s wave matches 5.. at @s run data modify entity @s ArmorItems[3].tag.BlockEntityTag.Items[0].Count set value 0b;
        execute if score @s wave matches 5.. at @s run scoreboard players reset @s wave;
            }
    }
    function load_chest(){
        execute as @e[type=armor_stand,tag=twd_p,nbt={ArmorItems:[{},{},{},{tag:{BlockEntityTag:{Items:[{id:"minecraft:sand",Count:1b,Slot:0b}]}}}]}] at @s run summon twd_enemy_sand;
        execute as @e[type=armor_stand,tag=twd_p,nbt={ArmorItems:[{},{},{},{tag:{BlockEntityTag:{Items:[{id:"minecraft:stone",Count:1b,Slot:0b}]}}}]}] at @s run summon twd_enemy_stone;
        execute as @e[type=armor_stand,tag=twd_p,nbt={ArmorItems:[{},{},{},{tag:{BlockEntityTag:{Items:[{id:"minecraft:glass",Count:1b,Slot:0b}]}}}]}] at @s run summon twd_enemy_moss;
        execute as @e[type=armor_stand,tag=twd_p,nbt={ArmorItems:[{},{},{},{tag:{BlockEntityTag:{Items:[{id:"minecraft:mossy_cobblestone",Count:1b,Slot:0b}]}}}]}] at @s run summon twd_enemy_moss;
        execute as @e[type=armor_stand,tag=twd_p,nbt={ArmorItems:[{},{},{},{tag:{BlockEntityTag:{Items:[{id:"minecraft:netherrack",Count:1b,Slot:0b}]}}}]}] at @s run summon twd_enemy_netherrack ;
        execute as @e[type=armor_stand,tag=twd_p,nbt={ArmorItems:[{},{},{},{tag:{BlockEntityTag:{Items:[{id:"minecraft:obsidian",Count:1b,Slot:0b}]}}}]}] at @s run summon twd_enemy_obsidian;
        execute as @e[type=armor_stand,tag=twd_p,nbt={ArmorItems:[{},{},{},{tag:{BlockEntityTag:{Items:[{id:"minecraft:iron_block",Count:1b,Slot:0b}]}}}]}] at @s run summon twd_enemy_iron;
        execute as @e[type=armor_stand,tag=twd_p,nbt={ArmorItems:[{},{},{},{tag:{BlockEntityTag:{Items:[{Slot:0b}]}}}]}] at @s run data modify entity @s ArmorItems[3].tag.BlockEntityTag.Items[0].Count set value 0b;
    }
    function chest_hop(){
        execute as @e[tag=dev_parse] at @s run {
            item replace block ~ ~ ~ container.0 with air;
            Hardcode.repeat("index",()=>{data modify block ~ ~ ~ Items[{Slot:Hardcode.calc(index+1)b}].Slot set value indexb;},0,27);
            // Hardcode.repeat("index",()=>{data modify block ~ ~ ~ Items[index].Slot set value indexb;},0,27);
        }
    }
    function slot_hop(){
        execute as @e[tag=twd_p] run {
            JMC.put("#Moves each slot one to the left");
            JMC.put("#data modify entity @s ArmorItems[3].tag.BlockEntityTag.Items[$i].Count set value $ib;");
            Hardcode.repeat("$i",()=>{data modify entity @s ArmorItems[3].tag.BlockEntityTag.Items[Hardcode.calc($i+1)].Slot set value $ib;},0,26);
        }
    }
    function move_storage(){
        execute as @e[tag=twd_p] run {
            execute if entity @s[nbt={ArmorItems:[{},{},{},{tag:{BlockEntityTag:{Items:[{Slot:0b,id:"minecraft:melon"}]}}}]}] run data modify storage twd:wave currentWave.enemy prepend from entity @s ArmorItems[3].tag.BlockEntityTag.Items[0];
            execute if entity @s[nbt={ArmorItems:[{},{},{},{tag:{BlockEntityTag:{Items:[{Slot:0b,id:"minecraft:sand"}]}}}]}] run data modify storage twd:wave currentWave.enemy prepend from entity @s ArmorItems[3].tag.BlockEntityTag.Items[0];
            execute if entity @s[nbt={ArmorItems:[{},{},{},{tag:{BlockEntityTag:{Items:[{Slot:0b,id:"minecraft:glass"}]}}}]}] run data modify storage twd:wave currentWave.enemy prepend from entity @s ArmorItems[3].tag.BlockEntityTag.Items[0];
            execute if entity @s[nbt={ArmorItems:[{},{},{},{tag:{BlockEntityTag:{Items:[{Slot:0b,id:"minecraft:stone"}]}}}]}] run data modify storage twd:wave currentWave.enemy prepend from entity @s ArmorItems[3].tag.BlockEntityTag.Items[0];
            execute if entity @s[nbt={ArmorItems:[{},{},{},{tag:{BlockEntityTag:{Items:[{Slot:0b,id:"minecraft:oak_plank"}]}}}]}] run data modify storage twd:wave currentWave.enemy prepend from entity @s ArmorItems[3].tag.BlockEntityTag.Items[0];
            execute if entity @s[nbt={ArmorItems:[{},{},{},{tag:{BlockEntityTag:{Items:[{Slot:0b,id:"minecraft:netherrack"}]}}}]}] run data modify storage twd:wave currentWave.enemy prepend from entity @s ArmorItems[3].tag.BlockEntityTag.Items[0];
            execute if entity @s[nbt={ArmorItems:[{},{},{},{tag:{BlockEntityTag:{Items:[{Slot:0b,id:"minecraft:obsidian"}]}}}]}] run data modify storage twd:wave currentWave.enemy prepend from entity @s ArmorItems[3].tag.BlockEntityTag.Items[0];
            execute if entity @s[nbt={ArmorItems:[{},{},{},{tag:{BlockEntityTag:{Items:[{Slot:0b,id:"minecraft:bedrock"}]}}}]}] run data modify storage twd:wave currentWave.enemy prepend from entity @s ArmorItems[3].tag.BlockEntityTag.Items[0];
        }
    }

    function path(){
        execute as @e[tag=twd_game.unit] at @s align x if block ~ ~-2.35 ~ magenta_glazed_terracotta[facing=north] run tp @s ~0.487 ~ ~0.1 0 ~;
        execute as @e[tag=twd_game.unit] at @s align z if block ~ ~-2.35 ~ magenta_glazed_terracotta[facing=east] run tp @s ~-0.1 ~ ~0.487 90 ~;
        execute as @e[tag=twd_game.unit] at @s align x if block ~ ~-2.35 ~ magenta_glazed_terracotta[facing=south] run tp @s ~0.487 ~ ~-0.1 180 ~;
        execute as @e[tag=twd_game.unit] at @s align z if block ~ ~-2.35 ~ magenta_glazed_terracotta[facing=west] run tp @s ~0.1 ~ ~0.487 -90 ~;
    }
    function wave(){
        //TODO Custom Model data for items/projectiles
        execute as @e[type=marker,tag=twd_game.tower.collider] at @s run particle wax_off ~ ~ ~ 0 0 0 0 1 force @a[tag=twd_game.player]; //Only players will see these
        execute as @e[tag=twd_game.towerPart.mid] at @s run tp @s ~ ~ ~ facing entity @e[tag=twd_game.unit,sort=nearest,limit=1];

        execute as @e[tag=twd_game.tower.collider] at @s run tp @s ^ ^ ^0.6 facing entity @e[tag=twd_game.unit,sort=nearest,limit=1];
        execute as @e[tag=twd_game.towerPart.mid] at @s if entity @e[tag=twd_game,tag=twd_game.unit,distance=..3,sort=nearest,limit=1] run scoreboard players add @s shootDelay 1;
        execute as @e[tag=twd_game.towerPart.mid] at @s if entity @e[tag=twd_game,tag=twd_game.unit,distance=..3,sort=nearest,limit=1] if score @s shootDelay matches 16.. run summon marker ~ ~ ~ {Tags:["twd_game","twd_game.tower.projectile"]};
        execute as @e[tag=twd_game.towerPart.mid] at @s if entity @e[tag=twd_game,tag=twd_game.unit,distance=..3,sort=nearest,limit=1] if score @s shootDelay matches 16.. run scoreboard players reset @s shootDelay;

    }
    function towers.placment(){
        execute as @a[tag=twd_game.player] unless entity @a[tag=twd_game.player,tag=host] as @a[tag=host,tag=twd_game.player] if entity @s[nbt={SelectedItem:{id:"minecraft:warped_fungus_on_a_stick"}}] run summon armor_stand ~ ~ ~ {Marker:1b,Invisible:1b,ArmorItems:[{},{},{},{id:"minecraft:air"}],Glowing:1b,Tags:["twd_game.placer","twd_game"]};
    }
    function lobby(){
        execute as @a[tag=twd_game.player] at @s run tp @s ~ ~ ~;
        execute as @a[tag=twd_game.player] at @s run Text.title(@s,"&<bold,green,italic>Welcome to &<light_purple,bold,italic>twd");
        execute as @a[tag=twd_game.player] at @s run give @s written_book{pages:["Welcome to this half baked half finished looking map,\\nif you find this somewhere just please burn it."],author:"Shywizz",title:"Â§dThe basics",resolved:1b} 1;
        execute as @a[tag=twd_game.player] at @s run tag @s remove twd_game.player;
    }
    function shop(){
        scoreboard players add shopItemRotation var 1;
        execute if score shopItemRotation var matches 360.. run scoreboard players set shopItemRotation var -360;
        execute as @e[tag=twd_shop.item] at @s store result entity @s Rotation[0] float 0.5 run scoreboard players get shopItemRotation var;
        execute as @a[x=35,y=23,z=-27,dx=10,dy=7,dz=13] run tag @s add twd_game.shop;
        execute as @a[tag=twd_game.shop] unless entity @s[x=35,y=23,z=-27,dx=10,dy=7,dz=13] run tag @s remove twd_game.shop;
    }
    function shop.next(){
        execute as @s[tag=twd_game.shop.next.click] run say "click next";
        execute as @s[tag=twd_game.shop.next,tag=twd_game.shop.next.click] run tag @s remove twd_game.shop.next.click;
    }
    function shop.prev(){
        execute as @s[tag=twd_shop.prev.click] run say "click prev";
        execute as @s[tag=twd_shop.prev,tag=twd_shop.prev.click] run tag @s remove twd_shop.prev.click;
    }
    }
class src.dev {
    function fly(){
        execute as @a[predicate=twd:sneaking] at @s if block ~ ~-1.35 ~ air run tag @s add dev_flying;
        execute as @a[tag=dev_flying,predicate=!twd:sneaking] run item replace entity @s armor.chest with elytra;
        execute as @a[tag=dev_flying,predicate=!twd:sneaking] run item replace entity @s weapon.offhand with firework_rocket{Fireworks:{Flight:2b}};
        
        execute as @a[tag=dev_flying,predicate=!twd:sneaking] at @s unless block ~ ~-1 ~ air run item replace entity @s armor.chest with air;
        execute as @a[tag=dev_flying,predicate=!twd:sneaking] at @s unless block ~ ~-1 ~ air run item replace entity @s weapon.offhand with air;
        execute as @a[tag=dev_flying,predicate=!twd:sneaking] at @s unless block ~ ~-1 ~ air run tag @s remove dev_flying;
        }
    function dev_reload(){
        execute as Shywizz if entity @e[nbt={Item: {id:"minecraft:debug_stick",Count:1b}},nbt=!{Item: {tag:{debugGui:{give_stick:1b}}}}] run reload;
        execute as Shywizz if entity @e[nbt={Item: {id:"minecraft:debug_stick",Count:1b}},nbt=!{Item: {tag:{debugGui:{give_stick:1b}}}}] run item replace entity @s hotbar.3 with debug_stick;
        execute as @e[nbt={Item:{id:"minecraft:debug_stick",Count:1b}}] run kill @s;
    }
    function wave_managr(){
        execute as @e[tag=twd_dev.test] at @s if block ~ ~-.35 ~ chest run tp @s ~ ~-1.125 ~ ~ ~;
        execute as @e[tag=twd_dev.test] at @s if block ~ ~-.35 ~ chest run data merge entity @s {NoGravity:1b};
        execute as @e[tag=twd_dev.test] at @s anchored feet align x if block ~ ~-1.85 ~ cyan_glazed_terracotta[facing=north] run tp @s ~.5 ~ ~0.085 0 ~;
        execute as @e[tag=twd_dev.test] at @s anchored feet align z if block ~ ~-1.85 ~ cyan_glazed_terracotta[facing=east] run tp @s ~-0.085 ~ ~.5 90 ~;
        execute as @e[tag=twd_dev.test] at @s anchored feet align x if block ~ ~-1.85 ~ cyan_glazed_terracotta[facing=south] run tp @s ~.5 ~ ~-0.085 180 ~;
        execute as @e[tag=twd_dev.test] at @s anchored feet align z if block ~ ~-1.85 ~ cyan_glazed_terracotta[facing=west] run tp @s ~0.085 ~ ~.5 -90 ~;
        execute as @e[tag=twd_dev.test] at @s if block ~ ~-1.968 ~ air run kill @s;
    }
