Player.onEvent(custom:talked_to_villager, ()=>{
            Raycast.simple(
              ()=>
                {
                if (entity @s[tag=twd_shop.next]){tag @s add twd_shop.next.click;}
                if (entity @s[tag=twd_shop.prev]){tag @s add twd_shop.prev.click;}

                },
              ()=>{},
              ()=>{},
              0.25,
              50,
              0.25
            );
          }
        );